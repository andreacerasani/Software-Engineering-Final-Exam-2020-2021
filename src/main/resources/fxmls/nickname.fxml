<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="720.0" prefWidth="1280.0" styleClass="menuBackground" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.GUI.Controller.NicknameController">
    <children>
        <Button fx:id="joinButton" minWidth="150.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="JOIN">
            <StackPane.margin>
                <Insets bottom="-300.0" />
            </StackPane.margin>
        </Button>
        <TextField fx:id="nickname" maxWidth="150.0">
            <StackPane.margin>
                <Insets bottom="100.0" />
            </StackPane.margin>
            <font>
                <Font size="16.0" />
            </font>
        </TextField>
        <Label text="Insert nickname">
            <font>
                <Font size="14.0" />
            </font>
            <StackPane.margin>
                <Insets bottom="300.0" />
            </StackPane.margin>
        </Label>
    </children>
    <stylesheets>
        <URL value="@../css/global.css" />
        <URL value="@../css/login_scene.css" />
    </stylesheets>
</StackPane>
