<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>


<StackPane prefHeight="400.0" prefWidth="600.0" styleClass="menuBackground" stylesheets="@../css/gameInterface.css" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.GUI.Controller.TakeFromMarketController">
   <children>
       <!-- content from market -->
       <fx:include fx:id="marketGrid" source="../fxmls/market.fxml" />
      <Label stylesheets="@../css/gameInterface.css" text="Select which row or column ">
         <StackPane.margin>
            <Insets bottom="300.0" />
         </StackPane.margin>
      </Label>
      <Button fx:id="doneButton" mnemonicParsing="false" stylesheets="@../css/gameInterface.css" text="DONE">
         <StackPane.margin>
            <Insets top="300.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
