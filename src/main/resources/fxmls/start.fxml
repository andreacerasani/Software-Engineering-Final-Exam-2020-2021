<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="720.0" prefWidth="1280.0" styleClass="startBackground" stylesheets="@../css/start.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.GUI.Controller.StartController">
   <children>
      <Button fx:id="onlineBtn" mnemonicParsing="false" prefHeight="84.0" prefWidth="136.0" styleClass="startButton" stylesheets="@../css/start.css" text="Online">
         <font>
            <Font size="24.0" />
         </font>
         <StackPane.margin>
            <Insets right="300.0" top="200.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="localBtn" mnemonicParsing="false" prefHeight="84.0" prefWidth="136.0" styleClass="startButton" stylesheets="@../css/start.css" text="Local">
         <font>
            <Font size="24.0" />
         </font>
         <StackPane.margin>
            <Insets left="300.0" top="200.0" />
         </StackPane.margin>
      </Button>
      <Label alignment="CENTER" styleClass="outline" stylesheets="@../css/start.css" text="Select Game Mode" textAlignment="CENTER" textFill="#4b89bd">
         <font>
            <Font size="45.0" />
         </font>
         <StackPane.margin>
            <Insets bottom="200.0" />
         </StackPane.margin>
      </Label>
   </children>
</StackPane>
